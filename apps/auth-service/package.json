{
  "name": "@ecommerce/auth-service",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "dependencies": {
    "express": "4.21.2",
    "sequelize": "6.37.5",
    "pg": "8.13.1",
    "pg-hstore": "2.3.4",
    "jsonwebtoken": "9.0.2",
    "argon2": "0.41.1",
    "amqplib": "0.10.5",
    "joi": "17.13.3",
    "helmet": "8.0.0",
    "cors": "2.8.5",
    "express-rate-limit": "7.5.0",
    "dotenv": "16.4.7",
    "winston": "3.17.0",
    "uuid": "11.0.4"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "18.19.68",
    "@types/cors": "2.8.17",
    "@types/jsonwebtoken": "9.0.7",
    "@types/amqplib": "0.10.5",
    "@types/uuid": "10.0.0",
    "@types/jest": "29.5.14",
    "@types/supertest": "6.0.2",
    "supertest": "7.0.0",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "ts-node-dev": "2.0.0",
    "typescript": "5.7.2",
    "sequelize-cli": "6.6.2",
    "sqlite3": "5.1.7"
  }
}
